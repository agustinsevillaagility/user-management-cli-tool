{
  "name": "auth0-org-cli",
  "version": "1.0.0",
  "description": "CLI tool for assigning users to Auth0 organizations with roles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "setup": "node setup.js",
    "build": "tsc",
    "cli": "npx ts-node src/cli.ts",
    "build-cli": "npm run build && node dist/cli.js",
    "add-arc-user": "npm run build-cli",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "npm run build"
  },
  "bin": {
    "add-arc-user": "./dist/cli.js"
  },
  "keywords": [
    "auth0",
    "management-api",
    "typescript",
    "wrapper"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/auth0": "^3.3.11",
    "auth0": "^4.31.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "dotenv": "^17.2.3",
    "inquirer": "^12.9.6"
  }
}
